<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tvTrack"
        android:layout_width="match_parent"
        android:layout_marginTop="@dimen/item_album_margin"
        android:layout_marginBottom="@dimen/item_album_margin"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-condensed"
        android:textSize="@dimen/track_description_textsize"
        android:textColor="@color/whiteOpaque"
        tools:text="Metalcore"/>

</FrameLayout>
